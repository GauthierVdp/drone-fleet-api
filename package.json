{
  "name": "drone-fleet-api",
  "version": "1.0.0",
  "description": "API for Drone Fleet Management",
  "main": "dist/index.js",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "node dist/rest/index.js",
    "dev": "ts-node-dev src/index.ts",
    "prisma": "prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/data/schema.prisma",
    "prisma:generate": "prisma generate --schema=src/data/schema.prisma",
    "prisma:seed": "ts-node src/data/seed.ts",
    "sequelize": "sequelize-cli --config ./src/config/database.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.4",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.5.2",
    "prisma": "^5.22.0",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.0",
    "@types/sequelize": "^4.28.20",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
