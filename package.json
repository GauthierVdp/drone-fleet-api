{
  "name": "drone-fleet-api",
  "version": "1.0.0",
  "description": "API for Drone Fleet Management",
  "main": "dist/index.js",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "prisma": "prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:seed": "ts-node src/data/seeders/seedAll.ts",
    "sequelize": "sequelize-cli --config ./src/config/database.ts",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.5.2",
    "prisma": "^5.22.0",
    "sequelize": "^6.37.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.0",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
